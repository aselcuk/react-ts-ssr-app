{
  "name": "react-ts-setup-app",
  "author": "Ali SELÃ‡UK",
  "version": "1.0.0",
  "description": "React & TypeScript based project setup app",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --env env=dev",
    "build": "webpack --config webpack.config.js --env env=prod && node build/server.js",
    "db": "json-server -p 4000 --watch db/db.json",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "lint:staged": "lint-staged",
    "svgr": "svgr ./src/public/assets/icons -d ./src/components/icons --replace-attr-values \"#000=currentColor,#fff=currentColor\" --icon --memo --ext tsx && npm run lint:fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/babel-plugin-remove-jsx-attribute": "^5.4.0",
    "@svgr/cli": "^5.5.0",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^5.0.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
